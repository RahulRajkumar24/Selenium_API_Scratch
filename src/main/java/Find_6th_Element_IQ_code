List<WebElement> elements = driver.findElements(By.xpath("//tagName")); // use your locator

// Always verify list size before accessing index
if (elements.size() >= 6) {
    WebElement sixthElement = elements.get(5);  // index starts from 0
    sixthElement.click();  // OR any action
} else {
    System.out.println("Less than 6 elements found!");
}




###  with Real Locator  ###

List<WebElement> menuItems = driver.findElements(By.cssSelector(".menu-item"));

if (menuItems.size() >= 6) {
    WebElement sixthItem = menuItems.get(5);
    System.out.println("6th item text: " + sixthItem.getText());
    sixthItem.click();
} else {
    System.out.println("Only " + menuItems.size() + " items found.");
}
